<h1>Listing clientes</h1>

<table>
  <thead>
    <tr>
      <th>Cli rut</th>
      <th>Cli nombre</th>
      <th>Cli apellidop</th>
      <th>Cli apellidom</th>
      <th>Cli telefono</th>
      <th>Cli direccion</th>
      <th>Cli numero</th>
      <th>Cli email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.cli_rut %></td>
        <td><%= cliente.cli_nombre %></td>
        <td><%= cliente.cli_apellidop %></td>
        <td><%= cliente.cli_apellidom %></td>
        <td><%= cliente.cli_telefono %></td>
        <td><%= cliente.cli_direccion %></td>
        <td><%= cliente.cli_numero %></td>
        <td><%= cliente.cli_email %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
