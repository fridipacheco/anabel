<h1>Listing detalle_compras</h1>

<table>
  <thead>
    <tr>
      <th>Dec cantidad</th>
      <th>Dec uni</th>
      <th>Dec total</th>
      <th>Dec descuento</th>
      <th>Dec producto seccion</th>
      <th>Dec compra</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_compras.each do |detalle_compra| %>
      <tr>
        <td><%= detalle_compra.dec_cantidad %></td>
        <td><%= detalle_compra.dec_uni %></td>
        <td><%= detalle_compra.dec_total %></td>
        <td><%= detalle_compra.dec_descuento %></td>
        <td><%= detalle_compra.producto_seccion.id %></td>
        <td><%= detalle_compra.compra.id %></td>
        <td><%= link_to 'Show', detalle_compra %></td>
        <td><%= link_to 'Edit', edit_detalle_compra_path(detalle_compra) %></td>
        <td><%= link_to 'Destroy', detalle_compra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle compra', new_detalle_compra_path %>
