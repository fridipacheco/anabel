<h1>Listing tipo_tarjets</h1>

<table>
  <thead>
    <tr>
      <th>Tta descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_tarjets.each do |tipo_tarjet| %>
      <tr>
        <td><%= tipo_tarjet.tta_descripcion %></td>
        <td><%= link_to 'Show', tipo_tarjet %></td>
        <td><%= link_to 'Edit', edit_tipo_tarjet_path(tipo_tarjet) %></td>
        <td><%= link_to 'Destroy', tipo_tarjet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo tarjet', new_tipo_tarjet_path %>
