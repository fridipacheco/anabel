<h1>Listing tipo_trabajadors</h1>

<table>
  <thead>
    <tr>
      <th>Tpt descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_trabajadors.each do |tipo_trabajador| %>
      <tr>
        <td><%= tipo_trabajador.tpt_descripcion %></td>
        <td><%= link_to 'Show', tipo_trabajador %></td>
        <td><%= link_to 'Edit', edit_tipo_trabajador_path(tipo_trabajador) %></td>
        <td><%= link_to 'Destroy', tipo_trabajador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo trabajador', new_tipo_trabajador_path %>
