<h1>Listing tipo_compras</h1>

<table>
  <thead>
    <tr>
      <th>Tco descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_compras.each do |tipo_compra| %>
      <tr>
        <td><%= tipo_compra.tco_descripcion %></td>
        <td><%= link_to 'Show', tipo_compra %></td>
        <td><%= link_to 'Edit', edit_tipo_compra_path(tipo_compra) %></td>
        <td><%= link_to 'Destroy', tipo_compra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo compra', new_tipo_compra_path %>
