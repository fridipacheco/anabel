<h1>Listing trabajadors</h1>

<table>
  <thead>
    <tr>
      <th>Tra rut</th>
      <th>Tra nombre</th>
      <th>Tra apellidop</th>
      <th>Tra apellidom</th>
      <th>Tra nacimiento</th>
      <th>Tra telefono</th>
      <th>Tra direccion</th>
      <th>Tra numero</th>
      <th>Tra email</th>
      <th>Tra sexo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trabajadors.each do |trabajador| %>
      <tr>
        <td><%= trabajador.tra_rut %></td>
        <td><%= trabajador.tra_nombre %></td>
        <td><%= trabajador.tra_apellidop %></td>
        <td><%= trabajador.tra_apellidom %></td>
        <td><%= trabajador.tra_nacimiento %></td>
        <td><%= trabajador.tra_telefono %></td>
        <td><%= trabajador.tra_direccion %></td>
        <td><%= trabajador.tra_numero %></td>
        <td><%= trabajador.tra_email %></td>
        <td><%= trabajador.tra_sexo %></td>
        <td><%= link_to 'Show', trabajador %></td>
        <td><%= link_to 'Edit', edit_trabajador_path(trabajador) %></td>
        <td><%= link_to 'Destroy', trabajador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trabajador', new_trabajador_path %>
