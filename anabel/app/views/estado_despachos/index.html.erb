<h1>Listing estado_despachos</h1>

<table>
  <thead>
    <tr>
      <th>Ede descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estado_despachos.each do |estado_despacho| %>
      <tr>
        <td><%= estado_despacho.ede_descripcion %></td>
        <td><%= link_to 'Show', estado_despacho %></td>
        <td><%= link_to 'Edit', edit_estado_despacho_path(estado_despacho) %></td>
        <td><%= link_to 'Destroy', estado_despacho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estado despacho', new_estado_despacho_path %>
