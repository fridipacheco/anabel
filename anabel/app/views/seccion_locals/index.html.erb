<h1>Listing seccion_locals</h1>

<table>
  <thead>
    <tr>
      <th>Local</th>
      <th>Seccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seccion_locals.each do |seccion_local| %>
      <tr>
        <td><%= seccion_local.local.loc_rs %></td>
        <td><%= seccion_local.seccion.sec_seccion %></td>
        <td><%= link_to 'Show', seccion_local %></td>
        <td><%= link_to 'Edit', edit_seccion_local_path(seccion_local) %></td>
        <td><%= link_to 'Destroy', seccion_local, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seccion local', new_seccion_local_path %>
